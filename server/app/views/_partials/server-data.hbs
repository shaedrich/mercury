<script>
	M.prop('initialPageView', true, true);
	M.prop('gaUrl', '{{server.gaUrl}}');
	M.prop('environment', '{{server.environment}}');
	M.prop('queryParams', {{{json queryParams}}});
	M.prop('tracking', {{{json tracking}}});
	M.prop('translations', {{{json (translations wikiVariables.language.content ns=settings.translationFiles)}}});
	M.prop('wikiaLanguage', '{{{wikiVariables.language.content}}}');
	M.prop('userId', {{userId}});
	M.prop('displayTitle', '{{displayTitle}}');
	M.prop('urlTitleParam', '{{urlTitleParam}}');
	M.prop('articleId', '{{articleId}}');
	M.prop('gaUserIdHash', '{{{gaUserIdHash}}}');
	M.prop('cookieDomain', {{{json settings.authCookieDomain}}});
	M.prop('servicesDomain', {{{json settings.servicesDomain}}});
	M.prop('openGraphBaseRoute', {{{json settings.openGraph.baseAPIPath}}});
	M.prop('imageReviewBaseRoute', {{{json settings.imageReview.baseAPIPath}}});
	M.prop('clickstream', {{{json settings.clickstream}}});
	M.provide('wiki', {{{json wikiVariables}}});
	{{#if server.mediawikiDomain}}
		M.prop('mediawikiDomain', '{{server.mediawikiDomain}}');
	{{/if}}
	M.prop('mediaWikiNamespace', '{{mediaWikiNamespace}}');
	{{#if exception}}
		M.prop('exception', {{{json exception}}}, true);
	{{/if}}
</script>
